ry } from 'react-router-dom';\nimport { FormHandles } from '@unform/core';\nimport { Form } from '@unform/web';\nimport * as Yup from 'yup';\n\nimport api from '../../services/api';\n\nimport { useToast } from '../../hooks/toast';\n\nimport getValidationErrors from '../../utils/getValidationsErros';\n\nimport goBarberLogo from '../../assets/logo.svg';\n\nimport Input from '../../components/Input';\nimport Button from '../../components/Button';\n\nimport { Container, Content, AnimationContainer, Background } from './styles';\n\ninterface SignUpFormData {\n  name: string;\n  email: string;\n  password: string;\n}\n\nconst SignUp: React.FC = () => {\n  const formRef = useRef<FormHandles>(null);\n  const { addToast } = useToast();\n  const history = useHistory();\n\n  const handleSubmit = useCallback(\n    async (data: SignUpFormData) => {\n      try {\n        formRef.current?.setErrors({});\n\n        const schema = Yup.object().shape({\n          name: Yup.string().required('Nome obrigatório'),\n          email: Yup.string()\n            .required('E-mail obrigatório')\n            .email('Digite um e-mail válido'),\n          password: Yup.string().min(6, 'No mínimo 6 dígitos'),\n        });\n\n        await schema.validate(data, {\n          abortEarly: false,\n        });\n\n        await api.post('/users', data);\n\n        history.push('/');\n\n        addToast({\n          type: 'success',\n          title: 'Cadastro realizado!',\n          description: 'Você já pode fazer seu logon no GoBarber',\n        });\n      } catch (e) {\n        if (e instanceof Yup.ValidationError) {\n          const errors = getValidationErrors(e);\n\n          formRef.current?.setErrors(errors);\n\n          return;\n        }\n\n        addToast({\n          type: 'error',\n          title: 'Erro no cadastro',\n          description: 'Ocorreu um erro ao fazer cadastro. Tente novamente',\n        });\n      }\n    },\n    [addToast, history],\n  );\n\n  return (\n    <Container>\n      <Background />\n      <Content>\n        <AnimationContainer>\n          <img src={goBarberLogo} alt=\"GoBarber\" />\n\n          <Form ref={formRef} onSubmit={handleSubmit}>\n            <h1>Faça seu cadastro</h1>\n\n            <Input name=\"name\" icon={FiUser} placeholder=\"Nome\" />\n            <Input name=\"email\" icon={FiMail} placeholder=\"E-mail\" />\n\n            <Input\n              name=\"password\"\n              icon={FiLock}\n              type=\"password\"\n              placeholder=\"Senha\"\n            />\n\n            <Button type=\"submit\">Cadastrar</Button>\n          </Form>\n\n          <Link to=\"/\">\n            <FiArrowLeft />\n            Voltar para logon\n          </Link>\n        </AnimationContainer>\n      </Content>\n    </Container>\n  );\n};\n\nexport default SignUp;\n","/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/utils/getValidationsErros.ts",["140"],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/components/Tooltip/index.tsx",[],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/components/Tooltip/styles.ts",[],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/components/ToastContainer/index.tsx",["141"],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/components/ToastContainer/styles.ts",[],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/hooks/toast.tsx",[],["142","143","144","145","146"],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/components/ToastContainer/Toast/index.tsx",["147","148"],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/components/ToastContainer/Toast/styles.ts",["149"],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/routes/index.tsx",[],"/Users/preis/Documents/Projetos/bootcamp/gobarberweb/src/routes/Route.tsx",["150"],"import React from 'react';\nimport {\n  Route as ReactDOMRoute,\n  RouteProps as ReactDOMRouteProps,\n  Redirect,\n} from 'react-router-dom';\n\nimport { useAuth } from '../hooks/auth';\n\ninte